101 DIALOGEX 0, 0, 150, 100
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Digite a Senha"
FONT 8, "MS Sans Serif"
BEGIN
    LTEXT "Senha:", -1, 10, 10, 50, 10
    EDITTEXT 102, 60, 10, 70, 14, ES_PASSWORD | WS_BORDER | WS_TABSTOP
    PUSHBUTTON "OK", IDOK, 30, 40, 40, 14, WS_TABSTOP
    PUSHBUTTON "Cancelar", IDCANCEL, 80, 40, 40, 14, WS_TABSTOP
END
